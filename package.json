{
  "name": "app",
  "private": true,
  "devDependencies": {
    "bootstrap-sass": "^3.4.3",
    "esbuild": "^0.25.12",
    "sass": "^1.93.3"
  },
  "scripts": {
    "build": "npx esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
    "build:css": "sass ./app/assets/stylesheets/application.scss:./app/assets/builds/application.css --no-source-map --load-path=node_modules --load-path=node_modules/bootstrap-sass/assets/stylesheets"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.20",
    "@tailwindcss/cli": "^4.1.16",
    "tailwindcss": "^4.1.16"
  }
}